# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
log/*.log
!/log/.keep
/tmp

# Ignore test reports and intermediate build artefacts
.sass-cache
/coverage
/doc
/vendor/ruby
/o-rdoc
/public/images/emoji

# Private and protected key files for Chamber
.chamber.pem
.chamber.pem.enc
